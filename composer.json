{
	"name": "wp-project-boilerplate",
	"type": "project",
	"license": "GPL-3.0-or-later",
	"require": {
		"php": ">=7.2",
		"oomphinc/composer-installers-extender": "*",
		"wpackagist-theme/twentytwenty":"*",
		"wpackagist-plugin/query-monitor":"*",
		"wpackagist-plugin/debug-bar":"*",
		"wpackagist-plugin/debug-bar-extender":"*",
		"wpackagist-plugin/debug-bar-cron":"*",
		"wpackagist-plugin/debug-bar-transients":"*",
		"wpackagist-plugin/log-deprecated-notices":"*",
		"wpackagist-plugin/rewrite-rulesinspector":"*",
	},
	"require-dev": {},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		}
	],
	"config": {
		"platform": {
			"php": "7.3"
		}
	},
	"extra": {
		"installer-types": [
			"library"
		],
		"installer-paths": {
			"content/mu-plugins/{$name}/": [
				"type:wordpress-muplugin"
			],
			"content/plugins/{$name}/": [
				"type:wordpress-plugin"
			],
			"content/{$name}/": [
				"type:wordpress-dropin"
			]
		}
	}
}
